<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:org.pepstock.charba.client"
	xmlns:s="urn:import:org.pepstock.charba.showcase.client.samples">

	<ui:style field="myStyle">
	.myLabel {
		cursor: pointer;
	}
	</ui:style>

	<g:HTMLPanel>
		<g:HorizontalPanel width="100%">
			<g:cell width="10%" verticalAlignment="top"
				horizontalAlignment="left">
				<g:VerticalPanel width="100%">
					<g:Label ui:field="home" styleName='{myStyle.myLabel}'>Home</g:Label>
					<!-- JSINTEROP -->
					<g:VerticalPanel width="100%">
						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Bar charts</g:header>
							<g:VerticalPanel spacing="4">
								<g:Label ui:field="barVertical"
									styleName='{myStyle.myLabel}'>Vertical</g:Label>
								<g:Label ui:field="barHorizontal"
									styleName='{myStyle.myLabel}'>Horizontal</g:Label>
								<g:Label ui:field="barMultiAxis"
									styleName='{myStyle.myLabel}'>Multi Axis</g:Label>
								<g:Label ui:field="barStacked"
									styleName='{myStyle.myLabel}'>Stacked</g:Label>
								<g:Label ui:field="barStackedGroups"
									styleName='{myStyle.myLabel}'>Stacked Groups</g:Label>
							</g:VerticalPanel>
						</g:DisclosurePanel>
						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Line charts</g:header>
							<g:VerticalPanel spacing="4">
								<g:Label ui:field="lineBasic"
									styleName='{myStyle.myLabel}'>Basic</g:Label>
								<g:Label ui:field="lineMultiAxis"
									styleName='{myStyle.myLabel}'>Multi Axis</g:Label>
								<g:Label ui:field="lineStepped"
									styleName='{myStyle.myLabel}'>Stepped</g:Label>
								<g:Label ui:field="lineInterpolation"
									styleName='{myStyle.myLabel}'>Interpolation</g:Label>
								<g:Label ui:field="lineStyles"
									styleName='{myStyle.myLabel}'>Line styles</g:Label>
								<g:Label ui:field="linePointStyles"
									styleName='{myStyle.myLabel}'>Point styles</g:Label>
								<g:Label ui:field="linePointStyleImages"
									styleName='{myStyle.myLabel}'>Point styles images</g:Label>
								<g:Label ui:field="linePointSize"
									styleName='{myStyle.myLabel}'>Point Size</g:Label>
							</g:VerticalPanel>
						</g:DisclosurePanel>
						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Area charts</g:header>
							<g:VerticalPanel spacing="4">
								<g:Label ui:field="areaBoundaries"
									styleName='{myStyle.myLabel}'>Boundaries (line)</g:Label>
								<g:Label ui:field="areaStacked"
									styleName='{myStyle.myLabel}'>Stacked (line)</g:Label>
							</g:VerticalPanel>
						</g:DisclosurePanel>
						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Other charts</g:header>
							<g:VerticalPanel spacing="4">
								<g:Label ui:field="scatter"
									styleName='{myStyle.myLabel}'>Scatter</g:Label>
								<g:Label ui:field="scatterMulti"
									styleName='{myStyle.myLabel}'>Scatter Multi Axis</g:Label>
								<g:Label ui:field="doughnut"
									styleName='{myStyle.myLabel}'>Doughnut</g:Label>
								<g:Label ui:field="pie"
									styleName='{myStyle.myLabel}'>Pie</g:Label>
								<g:Label ui:field="polar"
									styleName='{myStyle.myLabel}'>Polar Area</g:Label>
								<g:Label ui:field="radar"
									styleName='{myStyle.myLabel}'>Radar</g:Label>
								<g:Label ui:field="bubble"
									styleName='{myStyle.myLabel}'>Bubble</g:Label>
								<g:Label ui:field="combobarline"
									styleName='{myStyle.myLabel}'>Combo Bar Line</g:Label>

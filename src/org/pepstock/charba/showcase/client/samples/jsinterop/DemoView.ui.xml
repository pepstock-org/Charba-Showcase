						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Scale options</g:header>
							<g:VerticalPanel spacing="4">
								<g:Label ui:field="glinesdisplay"
									styleName='{myStyle.myLabel}'>Grid lines display</g:Label>
								<g:Label ui:field="glinesstyle"
									styleName='{myStyle.myLabel}'>Grid lines style</g:Label>
								<g:Label ui:field="multilabels"
									styleName='{myStyle.myLabel}'>Multilines labels</g:Label>
								<g:Label ui:field="filterlabels"
									styleName='{myStyle.myLabel}'>Filtering labels</g:Label>
								<g:Label ui:field="nomumericYaxis"
									styleName='{myStyle.myLabel}'>Non numeric Y Axis</g:Label>
							</g:VerticalPanel>
						</g:DisclosurePanel>
						<g:DisclosurePanel animationEnabled="true"
							open="false">
							<g:header>Legends</g:header>

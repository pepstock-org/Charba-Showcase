<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="downButton"
		type="org.pepstock.charba.showcase.client.resources.Images" />
	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%" height="100%">
			<g:cell width="90%" verticalAlignment="middle"
				horizontalAlignment="center">
				<g:SimplePanel ui:field="panel"></g:SimplePanel>
			</g:cell>
			<g:cell width="10%" verticalAlignment="top"
				horizontalAlignment="center">
				<g:HorizontalPanel spacing="5">
					<g:Button ui:field="back">Back</g:Button>
					<g:PushButton ui:field='source' enabled='true'>
						<g:upFace image='{downButton.github}' />
					</g:PushButton>
				</g:HorizontalPanel>
			</g:cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
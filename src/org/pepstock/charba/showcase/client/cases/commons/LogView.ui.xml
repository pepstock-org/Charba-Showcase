<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:org.pepstock.charba.client.gwt.widgets">

	<ui:style field="myLogs">
	.myCategory {
		cursor: pointer;
		color: #aaa;
		font-weight: 300;
		font-size: 1.5rem;
		padding-top: 12px;
	}
	</ui:style>

	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%" horizontalAlignment="left">
			<g:Label ui:field="titleLog" styleName='{myLogs.myCategory}' width="100%">Event logs</g:Label>
			<g:VerticalPanel ui:field="log" width="100%" horizontalAlignment="left"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 